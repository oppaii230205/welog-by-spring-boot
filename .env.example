# Environment Variables for Local Docker Development
# Copy this file to .env and customize the values

# Supabase PostgreSQL Database Configuration
DB_URL=jdbc:postgresql://db.xxxxxxxxxxxxx.supabase.co:5432/postgres
DB_USERNAME=postgres.xxxxxxxxxxxxx
DB_PASSWORD=your-supabase-database-password

# Supabase Storage Configuration
SUPABASE_URL=https://xxxxxxxxxxxxx.supabase.co
SUPABASE_KEY=your-supabase-anon-public-key

# JWT Configuration (IMPORTANT: Change in production!)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-at-least-32-characters
JWT_EXPIRATION_MS=86400000
